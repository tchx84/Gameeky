<?xml version='1.0' encoding='UTF-8'?>
<!-- Created with Cambalache 0.17.0 -->
<interface>
  <!-- interface-name confirmation_discard_window.ui -->
  <requires lib="libadwaita" version="1.2"/>
  <template class="ConfirmationCreateWindow" parent="AdwMessageDialog">
    <property name="body" translatable="yes">It's easier to create new projects from existing thematic packs. Browse GNOME Software for these Add-ons.</property>
    <property name="body-use-markup">True</property>
    <property name="close-response">cancel</property>
    <property name="default-response">browse</property>
    <property name="heading" translatable="yes">Create New Project?</property>
    <signal name="response" handler="on_response_clicked"/>
    <!-- Custom fragments -->
    <responses>
      <response id="cancel" translatable="yes">Cancel</response>
      <response id="create" translatable="yes">Create</response>
      <response id="browse" translatable="yes" appearance="suggested">Browse</response>
    </responses>
  </template>
</interface>
